{
  "name": "tadis-analyzer",
  "version": "1.1.3",
  "description": "generic analyzer tool for architecture discovery",
  "main": "build/src/lib.js",
  "types": "build/src/lib.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "author": "Andreas Blunk, MaibornWolff GmbH",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/app/default/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node build/src/app/default/main.js",
    "code-pattern": "ts-node -r tsconfig-paths/register src/app/code-pattern/main.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test-failed": "jest -f --runInBand",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker-build": "node docker-build.js",
    "docker-run": "node docker-run.js",
    "docker-publish": "node docker-publish.js",
    "publish": "tsc && yarn publish"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/common": "^7.5.3",
    "@nestjs/core": "^7.5.3",
    "@nestjs/platform-express": "7.5.3",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "immer": "^5.1.0",
    "joi": "^14.3.1",
    "kubernetes-client": "^9",
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.3",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.5.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^26.0.15",
    "@types/joi": "^14.3.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "jest": "^26.6.3",
    "nodemon": "^1.19.4",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
