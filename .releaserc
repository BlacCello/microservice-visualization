{
    "branches": [
        "master",
        "main",
        "dev"
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        [
            "@semantic-release/release-notes-generator",
            {
                "writerOpts": {
                    "footer": "The Docker image for this release is available at `ghcr.io/${{github.repository_owner}}/tadis-ui:${{nextRelease.version}}`."
                }
            }
        ],
        "@semantic-release/github",
        "@semantic-release/git"
    ]
}
